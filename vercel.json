{
    "version": 2,
    "builds": [
      {
        "src": "app.py", 
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/livraria/add",
        "dest": "/app.py" 
      },
      {
        "src": "/livraria",
        "dest": "/app.py"
      },
      {
        "src": "/livraria/<int:id>",
        "dest": "/app.py"
      },
      {
        "src": "/livraria/delete/<int:id>",
        "dest": "/app.py"
      },
      {
        "src": "/livraria/add",
        "dest": "/app.py"
      },
      {
        "src": "/livraria/listar",
        "dest": "/app.py"
      },
      {
        "src": "/(.*)", 
        "dest": "/app.py"
      }
    ]
  }

  
  
  
  